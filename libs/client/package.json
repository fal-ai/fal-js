{
  "name": "@fal-ai/client",
  "description": "The fal.ai client for JavaScript and TypeScript",
  "version": "1.8.4",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fal-ai/fal-js.git",
    "directory": "libs/client"
  },
  "keywords": [
    "fal",
    "client",
    "ai",
    "ml",
    "typescript"
  ],
  "exports": {
    ".": "./src/index.js",
    "./endpoints": "./src/types/endpoints.js",
    "./schemas": "./src/types/schemas.js"
  },
  "typesVersions": {
    "*": {
      "endpoints": [
        "src/types/endpoints.d.ts"
      ],
      "schemas": [
        "src/types/schemas.d.ts"
      ]
    }
  },
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "dependencies": {
    "@msgpack/msgpack": "^3.0.0-beta2",
    "eventsource-parser": "^1.1.2",
    "robot3": "^0.4.1"
  },
  "peerDependencies": {
    "zod": "^4"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
